  "name": "Espressif Generic ESP8266 ESP-01 4M",
  "upload": {
    "maximum_ram_size": 81920,
    "maximum_size": 4194304,
    "require_upload_port": true,
    "resetmethod": "ck",
    "speed": 115200
  },
